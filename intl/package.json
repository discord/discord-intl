{
  "name": "@discord/intl",
  "version": "0.1.3-rc.0",
  "license": "MIT",
  "description": "Client runtime for managing messages and translations in a React project.",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "exports": {
      ".": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "build:release": "tsc && cp ./src/*.d.ts dist/",
    "prepublishOnly": "pnpm build:release"
  },
  "dependencies": {
    "@formatjs/intl": "^2.10.1",
    "intl-messageformat": "^10.5.11"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.4.11",
    "typescript": "*"
  },
  "peerDependencies": {
    "@rspack/core": "0.x || 1.x",
    "webpack": "^5.0.0",
    "react": "> 16"
  },
  "peerDependenciesMeta": {
    "@rspack/core": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  }
}