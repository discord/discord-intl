{
  "name": "@discord/swc-intl-message-transformer",
  "license": "MIT",
  "version": "0.11.2-rc.3",
  "description": "SWC plugin for minifying intl message usages.",
  "author": "Jon Egeland",
  "keywords": [
    "swc-plugin"
  ],
  "main": "./swc_intl_message_transformer.wasm",
  "repository": {
    "type": "git",
    "url": "https://github.com/discord/discord-intl"
  },
  "scripts": {
    "build": "cargo build --target wasm32-unknown-unknown --release && cp ../../target/wasm32-unknown-unknown/release/swc_intl_message_transformer.wasm .",
    "build:dev": "cargo build --target wasm32-unknown-unknown && cp ../../target/wasm32-unknown-unknown/debug/swc_intl_message_transformer.wasm .",
    "prepublishOnly": "ls swc_intl_message_transformer.wasm"
  },
  "files": [
    "./swc_intl_message_transformer.wasm"
  ]
}